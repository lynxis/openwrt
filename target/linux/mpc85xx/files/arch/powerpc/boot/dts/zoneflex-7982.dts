/dts-v1/;

/include/ "fsl/p1020si-pre.dtsi"

/ {
	model = "Ruckus ZoneFlex 7982";
	compatible = "ruckus,zoneflex-7982";
	#address-cells = <0x2>;
	#size-cells = <0x2>;

	chosen {
		linux,stdout-path = "/soc@ffe00000/serial@4500";
		bootargs = "console=ttyS0,115200  uboot=1 usb  ";
	};

	aliases {
		led-boot = &system_yellow;
		led-failsafe = &system_yellow;
		led-running = &system_yellow;
		led-upgrade = &system_yellow;
	};

	memory {
		reg = <0x0 0x0 0x0 0x8000000>;
		device_type = "memory";
	};

	lbc: localbus@ffe05000 {
		reg = <0x0 0xffe05000 0 0x1000>;
		ranges = <0x0 0x0 0x0 0xef000000 0x01000000
			  0x1 0x0 0x0 0xff800000 0x00040000>;
	};

	soc: soc@ffe00000 {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges = <0x0 0x0 0xffe00000 0x100000>;

		spi@7000 {
			cell-index = <0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			espi,num-ss-bits = <0x4>;
			mode = "cpu";

			flash@0 {
				/* test with 40 mhz */
				compatible = "jedec,spi-nor";
				reg = <0>;
				spi-max-frequency = <25000000>;

				partitions {
					compatible = "ecoscentric,redboot-fis-partitions";
				};
			};
		};

		gpio0: gpio-controller@fc00 {
		};

		tdm@16000 {
			compatible = "fsl,starlite-tdm";
			reg = <0x16000 0x200 0x2c000 0x2000>;
			clock-frequency = <0x0>;
			interrupts = <0x3f 0x8 0x3e 0x8>;
			interrupt-parent = <0x2>;
		};

		l2-cache-controller@20000 {
			cache-level = <0x2>;
			cache-sets = <0x400>;
			cache-block-size = <0x20>;
			cache-unified;
			compatible = [66 73 6c 2c 70 31 30 31 31 2d 6c 32 2d 63 61 63 68 65 2d 63 6f 6e 74 72 6f 6c 6c 65 72 00 63 61 63 68 65 00 07 e7];
			reg = <0x20000 0x1000>;
			cache-line-size = <0x20>;
			cache-size = <0x40000>;
			interrupt-parent = <0x2>;
			interrupts = <0x10 0x2>;
			linux,phandle = <0x1>;
		};

		mdio@24000 {
			phy0: ethernet-phy@6 {
				interrupts = <3 1 0 0>;
				reg = <0x6>;
			};

			phy1: ethernet-phy@5 {
				/* todo check interupts. oem use cells: */
				interrupts = <1 1 0 0>;
				reg = <0x5>;
			};
		};

		mdio@25000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,etsec2-tbi";
			reg = <0x25000 0x1000 0xb1030 0x4>;

			tbi-phy@11 {
				reg = <0x11>;
				device_type = "tbi-phy";
			};
		};

		mdio@26000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,etsec2-tbi";
			reg = <0x26000 0x1000 0xb1030 0x4>;

			tbi-phy@11 {
				reg = <0x11>;
				device_type = "tbi-phy";
			};
		};

		ptimer@b0e00 {
			timer-frequency = <0x9ef21a9>;
			compatible = "fsl,gianfar-ptp-timer";
			reg = <0xb0e00 0xb0>;
			interrupts = <0x44 0x2 0x45 0x2 0x46 0x2>;
			interrupt-parent = <0x2>;
			tmr-prsc = <0x2>;
			cksel = <0x1>;
			linux,phandle = <0x4>;
		};

		message@42400 {
			compatible = "fsl,p1020-msg", "fsl,mpic-msg";
			reg = <0x42400 0x200>;
			interrupts = <0xb4 0x2 0xb5 0x2 0xb6 0x2 0xb7 0x2>;
			interrupt-parent = <0x2>;
		};

		power@e0070 {
			compatible = "fsl,mpc8548-pmc";
			reg = <0xe0070 0x20>;

			soc-clk@24 {
				fsl,pmcdr-mask = <0x80>;
				linux,phandle = <0x5>;
			};

			soc-clk@25 {
				fsl,pmcdr-mask = <0x40>;
			};

			soc-clk@26 {
				fsl,pmcdr-mask = <0x20>;
				linux,phandle = <0x7>;
			};
		};

		msi@41600 {
			compatible = "fsl,p1020-msi", "fsl,mpic-msi";
			reg = <0x41600 0x80>;
			msi-available-ranges = <0x0 0x100>;
			interrupts = <0xe0 0x0 0xe1 0x0 0xe2 0x0 0xe3 0x0 0xe4 0x0 0xe5 0x0 0xe6 0x0 0xe7 0x0>;
			interrupt-parent = <0x2>;
		};

		enet0: ethernet@b0000 {
			status = "okay";
			phy-handle = <&phy0>;
			phy-connection-type = "rgmii-id";
		};

		enet1: ethernet@b1000 {
			status = "disabled";
		};

		enet2: ethernet@b2000 {
			status = "okay";
			phy-handle = <&phy1>;
			phy-connection-type = "rgmii-id";
		};
	};

	pci0: pcie@ffe09000 {
		reg = <0x0 0xffe09000 0x0 0x1000>;
		ranges = <0x2000000 0x0 0xa0000000 0x0 0xa0000000 0x0 0x20000000
			  0x1000000 0x0 0x0 0x0 0xffc10000 0x0 0x10000>;

		pcie@0 {
			ranges = <0x2000000 0x0 0xa0000000
				  0x2000000 0x0 0xa0000000
				  0x0 0x20000000

				  0x1000000 0x0 0x0
				  0x1000000 0x0 0x0
				  0x0 0x100000>;
		};
	};

	pci1: pcie@ffe0a000 {
		reg = <0x0 0xffe0a000 0x0 0x1000>;
		ranges = <0x2000000 0x0 0x80000000 0x0 0x80000000 0x0 0x20000000
			  0x1000000 0x0 0x0 0x0 0xffc00000 0x0 0x10000>;

		pcie@0 {
			ranges = <0x2000000 0x0 0x80000000
				  0x2000000 0x0 0x80000000
				  0x0 0x20000000

				  0x1000000 0x0 0x0
				  0x1000000 0x0 0x0
				  0x0 0x100000>;
		};
	};

	leds {
		compatible = "gpio-leds";

		system_yellow: system {
			label = "zoneflex-7982:yellow:system";
			gpios = <&gpio0 486 1>;
		};
/*
		system_red {
			label = "zoneflex-7982:red:system";
			gpios = <&gpio0 489 1>;
		};
*/
	};

	buttons {
		compatible = "gpio-keys";

		reset {
			label = "Reset Button";
			linux,code = <0x198>;
			gpios = <&gpio0 490 1>;
		};
	};
};

/include/ "fsl/p1020si-post.dtsi"

